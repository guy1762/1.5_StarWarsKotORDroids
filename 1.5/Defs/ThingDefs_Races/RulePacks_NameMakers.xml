<Defs>

    <!-- This Rulepackdef is what finds the txt files to be used in the later rulepacks -->
    <RulePackDef>
        <defName>guy762_DroidNameRules</defName>
        <rulePack>
            <rulesFiles>
                <li>Droid_TwoCode->droidnames/astrocode</li>
                <li>Droid_ID->droidnames/genericID</li>
				<li>Droid_HK47->droidnames/HK47</li>
                <li>Droid_HK50->droidnames/HK50</li>
                <li>Droid_HK51->droidnames/HK51</li>
				<li>Droid_T3M4->droidnames/T3M4</li>
                <li>Droid_T3->droidnames/T3</li>
                <li>Droid_3C->droidnames/3C</li>
                <li>Droid_IT->droidnames/IT</li>
				<li>Droid_ADMk1->droidnames/ADMk1</li>
				<li>Droid_ADMk4->droidnames/ADMk4</li>
                <li>Droid_R8009->droidnames/R8009</li>
                <li>Droid_DevWD->droidnames/Devastator</li>
                <li>Droid_SentWD->droidnames/Sentinel</li>
                <li>Droid_GE3->droidnames/GE3</li>
                <li>Droid_KX12->droidnames/KX12</li>
                <li>Droid_KM1->droidnames/KM1</li>
                <li>Droid_MPD->droidnames/MPD</li>
                <li>Droid_GOTO->droidnames/GOTO</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>

    <!-- This is the item that goes into the racedef's namegenerator
        Example - <nameGenerator>ATR_AndroidMaleNames</nameGenerator> -->
